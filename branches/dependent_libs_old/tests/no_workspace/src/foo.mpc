project {
  Define_Custom(run) {
    command = echo
    inputext += .jim
    inputext += .joe
    inputext -= .joe, .jim

    optional(resource_outputext) {
      commandflags(-good && -bad ^^ -dumb) = .resource
    }
    optional(documentation_outputext) {
      commandflags(-good && -bad && ) = .rtf
      commandflags(&& -flair) += .rol
      commandflags(-good && -bad && ) += .blue
    }
  }

  Run_Files {
    poo.run
  }
}
