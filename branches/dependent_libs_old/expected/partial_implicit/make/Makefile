all: explicit-target implicit-target

clean depend generated realclean $(CUSTOM_TARGETS):
	@cd explicit && $(MAKE) -f Makefile.explicit $(@)
	@cd implicit && $(MAKE) -f Makefile.implicit $(@)

.PHONY: explicit-target
explicit-target:
	@cd explicit && $(MAKE) -f Makefile.explicit generated all

.PHONY: implicit-target
implicit-target:
	@cd implicit && $(MAKE) -f Makefile.implicit generated all

project_name_list:
	@echo explicit-target
	@echo implicit-target
