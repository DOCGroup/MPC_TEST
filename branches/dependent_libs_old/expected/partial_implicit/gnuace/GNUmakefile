MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: explicit-target implicit-target

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@cd explicit && $(MAKE) -f GNUmakefile.explicit $(@)
	@cd implicit && $(MAKE) -f GNUmakefile.implicit $(@)

.PHONY: explicit-target
explicit-target:
	@cd explicit && $(MAKE) -f GNUmakefile.explicit all

.PHONY: implicit-target
implicit-target:
	@cd implicit && $(MAKE) -f GNUmakefile.implicit all

project_name_list:
	@echo explicit-target
	@echo implicit-target
