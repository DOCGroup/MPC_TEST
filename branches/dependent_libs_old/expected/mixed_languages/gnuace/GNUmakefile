MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: base-target basic-target

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@cd base && $(MAKE) -f GNUmakefile.base $(@)
	@cd basic && $(MAKE) -f GNUmakefile.basic $(@)

.PHONY: base-target
base-target:
	@cd base && $(MAKE) -f GNUmakefile.base all

.PHONY: basic-target
basic-target: base-target
	@cd basic && $(MAKE) -f GNUmakefile.basic all

project_name_list:
	@echo base-target
	@echo basic-target
