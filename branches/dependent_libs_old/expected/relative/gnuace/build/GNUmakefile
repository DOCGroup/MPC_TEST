MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: build_lib build_exe

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@$(MAKE) -f GNUmakefile.build_lib $(@)
	@$(MAKE) -f GNUmakefile.build_exe $(@)

.PHONY: build_lib
build_lib:
	@$(MAKE) -f GNUmakefile.build_lib all

.PHONY: build_exe
build_exe: build_lib
	@$(MAKE) -f GNUmakefile.build_exe all

project_name_list:
	@echo build_exe
	@echo build_lib
