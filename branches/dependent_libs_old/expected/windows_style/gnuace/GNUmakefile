MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: support_lib sub-target

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@$(MAKE) -f GNUmakefile.support_lib $(@)
	@cd sub && $(MAKE) -f GNUmakefile.sub $(@)

.PHONY: support_lib
support_lib:
	@$(MAKE) -f GNUmakefile.support_lib all

.PHONY: sub-target
sub-target: support_lib
	@cd sub && $(MAKE) -f GNUmakefile.sub all

project_name_list:
	@echo support_lib
	@echo sub-target
