$(REMAINING_TARGETS):
	@$(MAKE) -f GNUmakefile.Monitor_Monitorlib $(@)
	@$(MAKE) -f GNUmakefile.NodeManager_stub $(@)
	@$(MAKE) -f GNUmakefile.NodeManager $(@)
	@$(MAKE) -f GNUmakefile.lib $(@)

