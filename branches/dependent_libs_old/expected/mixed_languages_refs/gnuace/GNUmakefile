MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: n-target r-target o-target

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@cd n && $(MAKE) -f GNUmakefile.n $(@)
	@cd r && $(MAKE) -f GNUmakefile.r $(@)
	@cd o && $(MAKE) -f GNUmakefile.o $(@)

.PHONY: n-target
n-target:
	@cd n && $(MAKE) -f GNUmakefile.n all

.PHONY: r-target
r-target:
	@cd r && $(MAKE) -f GNUmakefile.r all

.PHONY: o-target
o-target: n-target r-target
	@cd o && $(MAKE) -f GNUmakefile.o all

project_name_list:
	@echo n-target
	@echo o-target
	@echo r-target
