all: foo_b foo_a foo_c

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@$(MAKE) -f GNUmakefile.foo_b $(@)
	@$(MAKE) -f GNUmakefile.foo_a $(@)
