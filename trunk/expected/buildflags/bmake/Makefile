!ifndef CUSTOM_TARGETS
CUSTOM_TARGETS=_EMPTY_TARGET_
!endif

all: bfs_exe bfs_lib

clean:
	@cd source
	$(MAKE) -$(MAKEFLAGS) -f Makefile.bfs_exe.bmak clean
	@cd ../
	@cd source
	$(MAKE) -$(MAKEFLAGS) -f Makefile.bfs_lib.bmak clean
	@cd ../

generated:
	@cd source
	$(MAKE) -$(MAKEFLAGS) -f Makefile.bfs_exe.bmak generated
	@cd ../
	@cd source
	$(MAKE) -$(MAKEFLAGS) -f Makefile.bfs_lib.bmak generated
	@cd ../

realclean:
	@cd source
	$(MAKE) -$(MAKEFLAGS) -f Makefile.bfs_exe.bmak realclean
	@cd ../
	@cd source
	$(MAKE) -$(MAKEFLAGS) -f Makefile.bfs_lib.bmak realclean
	@cd ../

$(CUSTOM_TARGETS):
	@cd source
	$(MAKE) -$(MAKEFLAGS) -f Makefile.bfs_exe.bmak $(CUSTOM_TARGETS)
	@cd ../
	@cd source
	$(MAKE) -$(MAKEFLAGS) -f Makefile.bfs_lib.bmak $(CUSTOM_TARGETS)
	@cd ../

bfs_exe:
	@cd source
	$(MAKE) -$(MAKEFLAGS) -f Makefile.bfs_exe.bmak all
	@cd ../

bfs_lib:
	@cd source
	$(MAKE) -$(MAKEFLAGS) -f Makefile.bfs_lib.bmak all
	@cd ../

project_name_list:
