MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: good-target not_ghs-target

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@cd good && $(MAKE) -f GNUmakefile.good $(@)
	@cd not_ghs && $(MAKE) -f GNUmakefile.not_ghs $(@)

.PHONY: good-target
good-target:
	@cd good && $(MAKE) -f GNUmakefile.good all

.PHONY: not_ghs-target
not_ghs-target:
	@cd not_ghs && $(MAKE) -f GNUmakefile.not_ghs all

project_name_list:
	@echo good-target
	@echo not_ghs-target
