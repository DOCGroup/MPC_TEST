MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: cras aly

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@cd source && $(MAKE) -f GNUmakefile.cras $(@)
	@cd source && $(MAKE) -f GNUmakefile.aly $(@)

.PHONY: cras
cras:
	@cd source && $(MAKE) -f GNUmakefile.cras all

.PHONY: aly
aly: cras
	@cd source && $(MAKE) -f GNUmakefile.aly all

project_name_list:
	@echo aly
	@echo cras
