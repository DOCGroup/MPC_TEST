all: build_exe build_lib

clean depend generated realclean check-syntax $(CUSTOM_TARGETS):
	@cd build && $(MAKE) -f Makefile.build_exe $(@)
	@cd build && $(MAKE) -f Makefile.build_lib $(@)

.PHONY: build_exe
build_exe:
	@cd build && $(MAKE) -f Makefile.build_exe all

.PHONY: build_lib
build_lib: build_exe
	@cd build && $(MAKE) -f Makefile.build_lib all

project_name_list:
	@echo build_exe
	@echo build_lib
