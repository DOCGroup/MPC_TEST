all: build_exe build_lib

clean depend generated realclean $(CUSTOM_TARGETS):
	@cd build && $(MAKE) -f Makefile.build_exe $(@)
	@cd build && $(MAKE) -f Makefile.build_lib $(@)

.PHONY: build_exe
build_exe:
	@cd build && $(MAKE) -f Makefile.build_exe generated all

.PHONY: build_lib
build_lib:
	@cd build && $(MAKE) -f Makefile.build_lib generated all

project_name_list:
	@echo build_exe
	@echo build_lib
