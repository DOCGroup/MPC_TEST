MAKEFILE = GNUmakefile

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: build_lib build_exe

.PHONY: build_lib
build_lib:
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.build_lib

.PHONY: build_exe
build_exe: build_lib
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.build_exe

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)
$(REMAINING_TARGETS):
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.build_lib $(@)
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.build_exe $(@)

