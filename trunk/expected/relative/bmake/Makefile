!ifndef CUSTOM_TARGETS
CUSTOM_TARGETS=_EMPTY_TARGET_
!endif

all: build_exe build_lib

clean:
	@cd build
	$(MAKE) -$(MAKEFLAGS) -f Makefile.build_exe.bmak clean
	@cd ../
	@cd build
	$(MAKE) -$(MAKEFLAGS) -f Makefile.build_lib.bmak clean
	@cd ../

generated:
	@cd build
	$(MAKE) -$(MAKEFLAGS) -f Makefile.build_exe.bmak generated
	@cd ../
	@cd build
	$(MAKE) -$(MAKEFLAGS) -f Makefile.build_lib.bmak generated
	@cd ../

realclean:
	@cd build
	$(MAKE) -$(MAKEFLAGS) -f Makefile.build_exe.bmak realclean
	@cd ../
	@cd build
	$(MAKE) -$(MAKEFLAGS) -f Makefile.build_lib.bmak realclean
	@cd ../

$(CUSTOM_TARGETS):
	@cd build
	$(MAKE) -$(MAKEFLAGS) -f Makefile.build_exe.bmak $(CUSTOM_TARGETS)
	@cd ../
	@cd build
	$(MAKE) -$(MAKEFLAGS) -f Makefile.build_lib.bmak $(CUSTOM_TARGETS)
	@cd ../

build_exe:
	@cd build
	$(MAKE) -$(MAKEFLAGS) -f Makefile.build_exe.bmak all
	@cd ../

build_lib:
	@cd build
	$(MAKE) -$(MAKEFLAGS) -f Makefile.build_lib.bmak all
	@cd ../

project_name_list:
	@echo build_exe
	@echo build_lib
