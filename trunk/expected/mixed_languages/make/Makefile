all: base-target csharp-target basic-target

clean depend generated realclean $(CUSTOM_TARGETS):
	@cd base && $(MAKE) -f Makefile.base $(@)
	@cd csharp && $(MAKE) -f Makefile.csharp $(@)
	@cd basic && $(MAKE) -f Makefile.basic $(@)

.PHONY: base-target
base-target:
	@cd base && $(MAKE) -f Makefile.base generated all

.PHONY: csharp-target
csharp-target:
	@cd csharp && $(MAKE) -f Makefile.csharp generated all

.PHONY: basic-target
basic-target: base-target csharp-target
	@cd basic && $(MAKE) -f Makefile.basic generated all

