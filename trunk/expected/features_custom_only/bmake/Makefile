	@cd source
	$(MAKE) -$(MAKEFLAGS) -f Makefile.fco.bmak realclean
	@cd ../
	@cd source
	$(MAKE) -$(MAKEFLAGS) -f Makefile.fco_use.bmak realclean
	@cd ../

$(CUSTOM_TARGETS):
