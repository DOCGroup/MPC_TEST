MAKEFILE = GNUmakefile_testing

ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: foo bar

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@$(MAKE) -f GNUmakefile.foo_testing $(@)
	@$(MAKE) -f GNUmakefile.bar_testing $(@)

.PHONY: foo
foo:
	@$(MAKE) -f GNUmakefile.foo_testing all

.PHONY: bar
bar: foo
	@$(MAKE) -f GNUmakefile.bar_testing all

project_name_list:
	@echo bar
	@echo foo
