!ENDIF

!IF "$(CUSTOM_TARGETS)" == ""
CUSTOM_TARGETS=_EMPTY_TARGET_
!ENDIF

all: base-target basic-target

clean depend generated realclean $(CUSTOM_TARGETS):
	@cd base
	$(MAKE) /f Makefile.base.mak CFG="$(CFG)" $(@)
	@cd $(MAKEDIR)
	@cd basic
	$(MAKE) /f Makefile.basic.mak CFG="$(CFG)" $(@)
	@cd $(MAKEDIR)

base-target:
	@cd base
	$(MAKE) /f Makefile.base.mak CFG="$(CFG)" all
	@cd $(MAKEDIR)

basic-target: base-target
	@cd basic
	$(MAKE) /f Makefile.basic.mak CFG="$(CFG)" all
	@cd $(MAKEDIR)

project_name_list:
	@echo base-target
	@echo basic-target
