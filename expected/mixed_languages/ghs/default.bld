#!build
default:
	nobuild
	:cx_option=exceptions
	:cx_option=std_namespaces
	:language=cxx
	:config_setting=longlong
	:cx_mode=ansi
base/base.bld
	program
	:object_dir=base/.obj
	:libdirs=base
basic/basic.bld
	shared_library
	:config_setting=pic
	:object_dir=basic/.shobj
	:libdirs=basic
	:defines=BASIC_BUILD_DLL
