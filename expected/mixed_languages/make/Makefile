all: base-target basic-target

clean depend generated realclean $(CUSTOM_TARGETS):
	@cd base && $(MAKE) -f Makefile.base $(@)
	@cd basic && $(MAKE) -f Makefile.basic $(@)

.PHONY: base-target
base-target:
	@cd base && $(MAKE) -f Makefile.base generated all

.PHONY: basic-target
basic-target: base-target
	@cd basic && $(MAKE) -f Makefile.basic generated all

