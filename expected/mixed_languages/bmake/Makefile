!ifndef CUSTOM_TARGETS
CUSTOM_TARGETS=_EMPTY_TARGET_
!endif

all: base basic

clean:
	@cd base
	$(MAKE) -$(MAKEFLAGS) -f Makefile.base.bmak clean
	@cd ../
	@cd basic
	$(MAKE) -$(MAKEFLAGS) -f Makefile.basic.bmak clean
	@cd ../

generated:
	@cd base
	$(MAKE) -$(MAKEFLAGS) -f Makefile.base.bmak generated
	@cd ../
	@cd basic
	$(MAKE) -$(MAKEFLAGS) -f Makefile.basic.bmak generated
	@cd ../

realclean:
	@cd base
	$(MAKE) -$(MAKEFLAGS) -f Makefile.base.bmak realclean
	@cd ../
	@cd basic
	$(MAKE) -$(MAKEFLAGS) -f Makefile.basic.bmak realclean
	@cd ../

$(CUSTOM_TARGETS):
	@cd base
	$(MAKE) -$(MAKEFLAGS) -f Makefile.base.bmak $(CUSTOM_TARGETS)
	@cd ../
	@cd basic
	$(MAKE) -$(MAKEFLAGS) -f Makefile.basic.bmak $(CUSTOM_TARGETS)
	@cd ../

base:
	@cd base
	$(MAKE) -$(MAKEFLAGS) -f Makefile.base.bmak all
	@cd ../

basic: base
	@cd basic
	$(MAKE) -$(MAKEFLAGS) -f Makefile.basic.bmak all
	@cd ../

project_name_list:
	@echo base
	@echo basic
