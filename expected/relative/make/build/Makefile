all: build_exe build_lib

depend: build_exe-depend build_lib-depend

clean generated realclean check-syntax $(CUSTOM_TARGETS):
	@$(MAKE) -f Makefile.build_exe $(@)
	@$(MAKE) -f Makefile.build_lib $(@)

.PHONY: build_exe
build_exe:
	@$(MAKE) -f Makefile.build_exe all

.PHONY: build_exe-depend
build_exe-depend:
	@$(MAKE) -f Makefile.build_exe depend

.PHONY: build_lib
build_lib: build_exe
	@$(MAKE) -f Makefile.build_lib all

.PHONY: build_lib-depend
build_lib-depend:
	@$(MAKE) -f Makefile.build_lib depend

