!IF "$(CFG)" == ""
CFG=Win32 Debug
!MESSAGE No configuration specified. Defaulting to Win32 Debug.
!ENDIF

!IF "$(CUSTOM_TARGETS)" == ""
CUSTOM_TARGETS=_EMPTY_TARGET_
!ENDIF

all: build_exe build_lib

clean depend generated realclean $(CUSTOM_TARGETS):
	@cd build
	$(MAKE) /f Makefile.build_exe.mak CFG="$(CFG)" $(@)
	@cd $(MAKEDIR)
	@cd build
	$(MAKE) /f Makefile.build_lib.mak CFG="$(CFG)" $(@)
	@cd $(MAKEDIR)

build_exe:
	@cd build
	$(MAKE) /f Makefile.build_exe.mak CFG="$(CFG)" all
	@cd $(MAKEDIR)

build_lib:
	@cd build
	$(MAKE) /f Makefile.build_lib.mak CFG="$(CFG)" all
	@cd $(MAKEDIR)

project_name_list:
	@echo build_exe
	@echo build_lib
