#!build
default:
	nobuild
	:cx_option=exceptions
	:cx_option=std_namespaces
	:language=cxx
	:config_setting=longlong
	:cx_mode=ansi
support_lib.bld
	shared_library
	:config_setting=pic
	:object_dir=./.shobj
	:libdirs=.
	:defines=BASIC_BUILD_DLL
sub/sub.bld
	program
	:object_dir=sub/.obj
	:sysincdirs=sub/..
	:libdirs=sub/.
	:libdirs=../lib
	:libraries=-lsupport_lib
