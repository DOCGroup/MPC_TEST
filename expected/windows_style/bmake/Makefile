	@cd sub
	$(MAKE) -$(MAKEFLAGS) -f Makefile.sub.bmak realclean
	@cd ../

$(CUSTOM_TARGETS):
	$(MAKE) -$(MAKEFLAGS) -f Makefile.support_lib.bmak $(CUSTOM_TARGETS)
	@cd sub
	$(MAKE) -$(MAKEFLAGS) -f Makefile.sub.bmak $(CUSTOM_TARGETS)
	@cd ../

support_lib:
