ifeq ($(findstring k,$(MAKEFLAGS)),k)
  KEEP_GOING = -
endif

include $(ACE_ROOT)/include/makeinclude/macros.GNU

all: Code Set ROOT Factory

REMAINING_TARGETS := $(subst all, , $(TARGETS_NESTED:.nested=)) $(CUSTOM_TARGETS)

$(REMAINING_TARGETS):
	@$(MAKE) -f GNUmakefile.Code $(@)
	@$(MAKE) -f GNUmakefile.Set $(@)
	@$(MAKE) -f GNUmakefile.ROOT $(@)
	@$(MAKE) -f GNUmakefile.Factory $(@)

.PHONY: Code
Code:
	@$(MAKE) -f GNUmakefile.Code all

.PHONY: Set
Set:
	@$(MAKE) -f GNUmakefile.Set all

.PHONY: ROOT
ROOT:
	@$(MAKE) -f GNUmakefile.ROOT all

.PHONY: Factory
Factory: Code Set ROOT
	@$(MAKE) -f GNUmakefile.Factory all

