#!gbuild
primaryTarget=ppc_integrity.tgt
[Project]
	--one_instantiation_per_object
	:sourceDir=.
	--std
	-language=cxx
	--long_long
	--new_style_casts
simple.gpj		[Shared Object]
	-pic
	-object_dir=./.shobj
	-DUSING_PCH
	-I$(IDL2JNI_ROOT)\runtime
	-I$(JAVA_HOME)\include
	-I$(JAVA_HOME)\include\$(JAVA_PLATFORM)
	-L.
	-L$(IDL2JNI_ROOT)\lib
	-lidl2jni_runtime
