#!build
default:
	nobuild
	:cx_option=exceptions
	:cx_option=std_namespaces
	:language=cxx
	:config_setting=longlong
	:cx_mode=ansi
source/bfs_exe.bld
	program
	:object_dir=source/.obj
	:libdirs=source
source/bfs_lib.bld
	shared_library
	:config_setting=pic
	:object_dir=source/.shobj
	:libdirs=source
